<?xml version="1.0" ?>

<!-- Base elements for system layer 
     -->

<!DOCTYPE node SYSTEM "../etc/objspec.dtd"> 

<node id="SysComps" parent="Elem">

    <node id="ConnPoint" parent="Vert">
	<node id="Provided" parent="Prop"></node>
	<node id="Required" parent="Prop"></node>
    </node>

    <!-- Element that acts as feedthrough for capsula of system - it extend
	 the connection from outside into the system
	 -->
    <node id="Extender" parent="Vert:ConnPoint"> 
	<node id="Int" parent="../Vert:ConnPoint"></node>
    </node>


</node>

